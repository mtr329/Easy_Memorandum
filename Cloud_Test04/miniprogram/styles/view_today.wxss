.todayCell{
    display: flex;
    box-sizing: border-box;
    align-items: center;
    width: 100%;
    height: 15%;
    /* border-width: 1px;
    border-color: black;
    border-style: solid; */
}
.todayDisplay{
    /* display: flex;
    flex-direction: column; */
    width: 100%;
    height: 90%;
    overflow: scroll;
}
.todayItem{
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 80%;
    background-color: white;
    border-radius: 50rpx;
}
.todayItemText{
    display: flex;
    justify-content: center;
    height: 80%;
    width:50%;
    align-items: center;
    font-weight: bold;
}
.todayItemBtns{
    display:flex;
    flex-direction: row;
    align-items: center;
    height: 80%;
    width: 45%;
}
.todayItemBtn{
    height:75% !important;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 100%;
}
